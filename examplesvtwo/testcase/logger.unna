// Logger module with state

var logCount = 0;

function logInfo(msg) {
    print("[INFO] " + msg);
    logCount = logCount + 1;
}

function logWarn(msg) {
    print("[WARN] " + msg);
    logCount = logCount + 1;
}

function logError(msg) {
    print("[ERROR] " + msg);
    logCount = logCount + 1;
}

function getLogCount() {
    return logCount;
}

// Pure computational loop test - no global variables
// This should be fully JIT compilable

function testLoop() {
    var sum = 0;
    var i = 0;
    
    // Simple loop - should trigger JIT compilation after 100 iterations
    while (i < 10000000) {
        sum = sum + i;
        i = i + 1;
    }
    
    return sum;
}

// Call the function
var result = testLoop();
print("Result: ");
print(result);

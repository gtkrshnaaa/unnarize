// Benchmark 3: Nested loops
// Tests control flow performance

print("=== Benchmark 3: Nested Loops ===");
print("Running nested iterations...");
print("");

var outer = 1000;
var inner = 1000;
var sum = 0;
var i = 0;

while (i < outer) {
    var j = 0;
    while (j < inner) {
        sum = sum + 1;
        j = j + 1;
    }
    i = i + 1;
}

print("Completed!");
print("Result: " + sum);

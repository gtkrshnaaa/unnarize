// Disable JIT temporarily to test interpreter baseline
// Set jitThreshold very high so JIT doesn't trigger

function testLoop() {
    var sum = 0;
    var i = 0;
    
    while (i < 100) {
        sum = sum + i;
        i = i + 1;
    }
    
    return sum;
}

var result = testLoop();
print("Result: ");
print(result);
print("Expected: 4950");

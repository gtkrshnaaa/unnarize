// 07_foreach.unna
print("=== Foreach Loop ===");
var arr = [10, 20, 30];
var sum = 0;

print("Iterating:");
for (var item : arr) {
    print("- " + item);
    sum += item;
}
print("Sum: " + sum);

print("=== Nested Foreach ===");
var matrix = [
    [1, 2],
    [3, 4]
];

for (var row : matrix) {
    print("Row:");
    for (var val : row) {
        print(" " + val);
    }
}

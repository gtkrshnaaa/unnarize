struct Status { status; uptime; features; }
function handleStatus(req) {
    print("Handled status route.");
    var s = Status("ok", 100, ["routing", "json"]);
    return ucoreHttp.json(s);
}

function handleHome(req) {
    return "Welcome to Unnarize Server!";
}

print("Registering routes...");
ucoreHttp.route("GET", "/api/status", "handleStatus");
ucoreHttp.route("GET", "/", "handleHome");

print("Starting server on 8081...");
ucoreHttp.listen(8081);
